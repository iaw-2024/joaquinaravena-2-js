<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Formaci√≥n</title>
    <link rel="stylesheet" href="style.css">
</head>
<body>
    <header>
        <h1>Plantel</h1>
    </header>
    <main>
        <section class="column">
            <ul class="posicion" id="arqueros">Arqueros
                <% arqueros.forEach(arquero => { %>
                    <li><%= arquero %></li>
                <% }); %>   
            </ul>
            <ul class="posicion" id="defensores">Defensores
                <% defensores.forEach(defensor => { %>
                    <li><%= defensor %></li>
                <% }); %>   
            </ul>
        </section>
        <section class="column">
            <ul class="posicion" id="mediocampistas">Mediocampistas
                <% mediocampistas.forEach(mediocampista => { %>
                    <li><%= mediocampista %></li>
                <% }); %>
            </ul>
            <ul class="posicion" id="delanteros">Delanteros
                <% delanteros.forEach(delantero => { %>
                    <li><%= delantero %></li>
                <% }); %>
            </ul>
        </section>      
    </main>
    <footer>
        <a href="/">Volver</a>
    </footer>
</body>
</html>